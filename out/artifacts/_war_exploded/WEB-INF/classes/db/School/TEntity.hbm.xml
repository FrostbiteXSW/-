<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="db.School.TEntity" table="T" schema="School">
        <id name="gh">
            <column name="gh" sql-type="char(4)" length="4"/>
        </id>
        <property name="xm">
            <column name="xm" sql-type="varchar(20)" length="20"/>
        </property>
        <property name="xb">
            <column name="xb" sql-type="char(1)" length="1"/>
        </property>
        <property name="csrq">
            <column name="csrq" sql-type="char(8)" length="8"/>
        </property>
        <property name="xl">
            <column name="xl" sql-type="varchar(10)" length="10"/>
        </property>
        <property name="jbgz">
            <column name="jbgz" sql-type="decimal(6,2)" precision="6" scale="2"/>
        </property>
        <property name="yxh">
            <column name="yxh" sql-type="char(2)"/>
        </property>
    </class>
</hibernate-mapping>