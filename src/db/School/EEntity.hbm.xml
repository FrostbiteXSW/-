<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="db.School.EEntity" table="E" schema="School">
        <composite-id>
            <key-property name="xh">
                <column name="xh" sql-type="char(4)" length="4"/>
            </key-property>
            <key-property name="xq">
                <column name="xq" sql-type="char(11)" length="11"/>
            </key-property>
            <key-property name="kh">
                <column name="kh" sql-type="char(8)" length="8"/>
            </key-property>
        </composite-id>
        <property name="gh">
            <column name="gh" sql-type="char(4)"/>
        </property>
        <property name="pscj">
            <column name="pscj" sql-type="int(3)" not-null="true"/>
        </property>
        <property name="kscj">
            <column name="kscj" sql-type="int(3)" not-null="true"/>
        </property>
        <property name="zpcj">
            <column name="zpcj" sql-type="int(3)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>