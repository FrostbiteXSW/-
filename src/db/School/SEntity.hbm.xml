<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="db.School.SEntity" table="S" schema="School">
        <id name="xh">
            <column name="xh" sql-type="char(4)" length="4"/>
        </id>
        <property name="xm">
            <column name="xm" sql-type="varchar(10)" length="10"/>
        </property>
        <property name="xb">
            <column name="xb" sql-type="char(1)" length="1"/>
        </property>
        <property name="csrq">
            <column name="csrq" sql-type="char(8)" length="8"/>
        </property>
        <property name="jg">
            <column name="jg" sql-type="varchar(10)" length="10"/>
        </property>
        <property name="sjhm">
            <column name="sjhm" sql-type="varchar(11)" length="11"/>
        </property>
    </class>
</hibernate-mapping>