<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="db.School.DEntity" table="D" schema="School">
        <id name="yxh">
            <column name="yxh" sql-type="char(2)" length="2"/>
        </id>
        <property name="mc">
            <column name="mc" sql-type="varchar(20)" length="20"/>
        </property>
        <property name="dz">
            <column name="dz" sql-type="varchar(50)" length="50"/>
        </property>
        <property name="lxdh">
            <column name="lxdh" sql-type="char(8)" length="8"/>
        </property>
    </class>
</hibernate-mapping>